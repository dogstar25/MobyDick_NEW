        },
        {
            "name": "sdl2-mixer",
            "version": "2.8.0#1"
        },
        {
            "name": "box2d",
            "version": "2.4.1#2"
        },
        {
            "name": "yaml-cpp",
            "version": "0.8.0#1"
        },
        {
            "name": "jsoncpp",
            "version": "1.9.5"
        },
        {
            "name": "imgui",
            "version": "1.88#1"
        },
        {
            "name": "glm",
            "version": "0.9.9.8#1"
        }

    ],

    "builtin-baseline": "5787cfa699a75805ef41938ec66bc7492714d290"

