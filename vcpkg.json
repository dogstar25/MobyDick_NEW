{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "mobydick",
  "version": "1.0.0",
    "dependencies": [
        {
            "name": "sdl2",
            "default-features": false
        },
        {
            "name": "sdl2-mixer",
            "default-features": false,
            "features": [ "mpg123" ]
        },
        {
            "name": "sdl2-image",
            "default-features": false
        },
        "sdl2-ttf",
        "box2d",
        "enkits",
        "jsoncpp",
        {
            "name": "imgui",
            "default-features": true,
            "features": [
                "opengl3-binding",
                "sdl2-binding",
                "sdl2-renderer-binding"
            ]
        },
        "glm",
        {
            "name": "glad",
            "default-features": true,
            "features": [
                "loader"
            ]
        }
    ],

    "overrides": [
        {
            "name": "sdl2",
            "version": "2.32.10"
        },
        {
            "name": "sdl2-image",
            "version": "2.6.3#1"
        },
        {
            "name": "sdl2-mixer",
            "version": "2.8.0#1"
        },
        {
            "name": "box2d", 
            "version": "3.1.1"
        },
        {
            "name": "yaml-cpp",
            "version": "0.8.0#1"
        },
        {
            "name": "jsoncpp",
            "version": "1.9.5"
        },
        {
            "name": "imgui",
            "version": "1.88#1"
        },
        {
            "name": "glm",
            "version": "0.9.9.8#1"
        },
        {
            "name": "enkits",
            "version": "1.11#3"
        }
    ],
  "builtin-baseline": "efcfaaf60d7ec57a159fc3110403d939bfb69729"
}
